{
  "expo": {
    "name": "DatumGerenciaApp",
    "slug": "DatumGerenciaApp",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.2.6",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
