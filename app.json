    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.datumposition.gerencia",
      "permissions": ["CAMERA", "WRITE_SETTINGS"],
      "versionCode": 6
    }
  }
}
